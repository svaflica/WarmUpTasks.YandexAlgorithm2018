import java.util.Scanner;

public static class CutSurface {
  private static int[] getCenter(Scanner scanner) {
    int type;
    int[] coords = new int[8];
    int[] res = new int[2];

    type = scanner.nextInt();
    if (type == 0) {
      for (int i = 0; i < 3; i++) {
        coords[i] = scanner.nextInt();
      }
      res[0] = coords[1] * 2;
      res[1] = coords[2] * 2;
    }
    else {
      for (int i = 0; i < 8; i++) {
        coords[i] = scanner.nextInt();
      }
      res[0] = coords[0] + coords[4];
      res[1] = coords[3] + coords[7];
    }

    return res;
  }

  public static void cutSurface(String[] args) {
    int n;
    boolean res = true;
    int[] lastCenter, preLastCenter, tempCenter;
    Scanner scanner = new Scanner(System.in);

    n = scanner.nextInt();
    if (n > 2) {
      preLastCenter = getCenter(scanner);
      lastCenter = getCenter(scanner);
      for (int i = 2; i < n; i++) {
        tempCenter = getCenter(scanner);
        if (Math.abs(tempCenter[0] - lastCenter[0]) == Math.abs(lastCenter[0] - preLastCenter[0]) &&
            Math.abs(tempCenter[1] - lastCenter[1]) == Math.abs(lastCenter[1] - preLastCenter[1]))
          continue;
        else {
          res = false;
          break;
        }
      }
    }
    if (res)
      System.out.println("Yes");
    else
      System.out.println("No");
  }
}
